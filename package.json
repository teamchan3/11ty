{
  "type": "module",
  "name": "my-11ty-project",
  "version": "1.0.0",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:build": "rimraf build",
    "build:eleventy": "eleventy --config=eleventy.config.mjs",
    "build:tailwind": "postcss src/css/tailwind.css -o dist/css/style.css",
    "build": "npm run clean:dist && cross-env ELEVENTY_ENV=development npm run build:eleventy",
    "build:prod": "npm run clean:build && cross-env ELEVENTY_ENV=production npm run build:eleventy",
    "dev": "npm run clean:dist && cross-env ELEVENTY_ENV=development eleventy --config=eleventy.config.mjs --serve",
    "start": "npm run dev"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "concurrently": "^6.4.0",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "esbuild": "^0.24.0",
    "html-minifier": "^4.0.0",
    "imagemin": "^9.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "js-beautify": "^1.15.1",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1"
  }
}
